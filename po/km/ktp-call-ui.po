#: src/approver.cpp:38
msgid "Incoming video call"
msgstr "មាន​ការ​ហៅ​ជា​វីដេអូ"

#: src/approver.cpp:39
#, kde-format
msgid "<p>%1 is asking you for a video call</p>"
msgstr "<p>%1 កំពុង​ស្នើសុំ​អ្នក​សម្រាប់​ការ​ហៅ​ជា​វីដេអូ</p>"

#: src/approver.cpp:41
msgid "Incoming call"
msgstr "កំពុង​ហៅ​ចូល"

#: src/approver.cpp:42
#, kde-format
msgid "<p>%1 is calling you</p>"
msgstr "<p>%1 កំពុង​ហៅ​អ្នក</p>"

#: src/approver.cpp:58 src/approver.cpp:80
msgctxt "action, answers the incoming call"
msgid "Answer"
msgstr "ឆ្លើយ"

#: src/approver.cpp:59 src/approver.cpp:82
msgctxt "action, rejects the incoming call"
msgid "Reject"
msgstr "បដិសេធ"

#: src/call-window.cpp:98
#, kde-format
msgid "Call with %1"
msgstr "ហៅ​ជាមួយ %1"

#: src/call-window.cpp:112
msgctxt "@info:status"
msgid "Connecting..."
msgstr "កំពុង​តភ្ជាប់..."

#: src/call-window.cpp:116
msgctxt "@info:status the remote user's phone is ringing"
msgid "Ringing..."
msgstr "កំពុង​រោទ៍..."

#: src/call-window.cpp:120
msgctxt "@info:status"
msgid "Remote user accepted the call, waiting for connection..."
msgstr "អ្នកប្រើ​ពី​ចម្ងាយ​ព្រម​ទទួល​ការ​ហៅ និង​កំពុង​រង់ចាំ​ការ​តភ្ជាប់..."

#: src/call-window.cpp:123 src/call-window.cpp:516
msgctxt "@info:status"
msgid "Talking..."
msgstr "កំពុង​ជជែក..."

#: src/call-window.cpp:144
msgctxt "@info:status"
msgid "Remote contact ended the call."
msgstr "ទំនាក់ទំនង​ពី​ចម្ងាយ​បាន​បញ្ចប់​ការ​ហៅ ។"

#: src/call-window.cpp:151
msgctxt "@info:status"
msgid "Remote contact rejected the call."
msgstr "ទំនាក់ទំនង​ពី​ចម្ងាយ​បាន​បដិសេធ​ការ​ហៅ ។"

#: src/call-window.cpp:156
msgctxt "@info:status"
msgid "Remote contact didn't answer."
msgstr "ទំនាក់ទំនង​ពី​ចម្ងាយ​មិន​បាន​ឆ្លើយ​ទេ ។"

#: src/call-window.cpp:160
msgctxt "@info:status"
msgid "Invalid number or contact."
msgstr "លេខ ឬ​ទំនាក់ទំនង​មិន​ត្រឹមត្រូវ ។"

#: src/call-window.cpp:164
msgctxt "@info:status"
msgid "You don't have enough balance to make this call."
msgstr "អ្នក​មិន​មាន​សមតុល្យ​គ្រប់គ្រាន់​សម្រាប់​ការ​ហៅ​នេះ​ទេ ។"

#: src/call-window.cpp:166
msgctxt "@info:status"
msgid "You don't have permission to make this call."
msgstr "អ្នក​គ្មាន​សិទ្ធិ​ក្នុង​ការ​ហៅ​ទេ ។"

#: src/call-window.cpp:171
msgctxt "@info:status"
msgid "Remote contact is currently busy."
msgstr "ទំនាក់ទំនង​ពី​ចម្ងាយ​កំពុង​ជាប់​រវល់ ។"

#: src/call-window.cpp:175
msgctxt "@info:status"
msgid "Internal component error."
msgstr "កំហុស​សមាសភាគ​ខាងក្នុង ។"

#: src/call-window.cpp:178
msgctxt "@info:status"
