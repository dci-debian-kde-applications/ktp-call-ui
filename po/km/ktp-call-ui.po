msgstr "កំហុស​បណ្ដាញ ។"

#: src/call-window.cpp:196
msgctxt "@info:status"
msgid "Failed to negotiate codecs."
msgstr "បាន​បរាជ័យ​ក្នុង​ការ​ចរចា​កូដិក ។"

#: src/call-window.cpp:198
msgctxt "@info:status"
msgid "Unsupported media type."
msgstr "មិន​គាំទ្រ​ប្រភេទ​មេឌៀ ។"

#: src/call-window.cpp:202
msgctxt "@info:status"
msgid "Connectivity error."
msgstr "កំហុស​ការ​តភ្ជាប់ ។"

#: src/call-window.cpp:206
msgctxt "@info:status"
msgid "Unknown error."
msgstr "មិន​ស្គាល់​កំហុស ។"

#: src/call-window.cpp:211
msgctxt "@info:status"
msgid "Disconnected."
msgstr "បាន​ផ្ដាច់ ។"

